{
  "name": "expoapp",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "run:android": "expo run:android",
    "run:ios": "expo run:ios",
    "storybook-generate": "sb-rn-get-stories --config-path=./.storybook",
    "storybook-watch": "sb-rn-watcher",
    "storybook": "cross-env STORYBOOK_ENABLED=1 expo start",
    "storybook:web": "storybook dev -p 6006 -c .storybook-web",
    "storybook:docs": "storybook dev -p 6006 -c .storybook-web --docs",
    "build-storybook": "storybook build -c .storybook-web",
    "create:component": "node ./scripts/create-component/index.mjs && yarn storybook-generate",
    "create:screen": "node ./scripts/create-screen/index.mjs",
    "create:navigator": "node ./scripts/create-navigator/index.mjs",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install",
    "generate:migration": "drizzle-kit generate:sqlite"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/metro-runtime": "~3.1.1",
    "@notifee/react-native": "^7.8.2",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/messaging": "^18.7.3",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/native": "^6.1.14",
    "@shopify/flash-list": "1.6.3",
    "@types/react": "~18.2.14",
    "axios": "^1.6.5",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.29.3",
    "expo": "~50.0.6",
    "expo-build-properties": "~0.11.0",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.7",
    "expo-file-system": "~16.0.6",
    "expo-font": "~11.10.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.26.4",
    "expo-sqlite": "~13.2.2",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-code-push": "^8.2.1",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "^1.10.0",
    "react-native-mmkv": "^2.12.1",
    "react-native-pager-view": "6.2.3",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-web": "~0.19.6",
    "swr": "^2.2.4",
    "yup": "^1.3.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@faker-js/faker": "^8.3.1",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/slider": "4.4.2",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-ondevice-actions": "^7.6.10",
    "@storybook/addon-ondevice-backgrounds": "^7.6.10",
    "@storybook/addon-ondevice-controls": "^7.6.10",
    "@storybook/addon-ondevice-notes": "^7.6.10",
    "@storybook/addon-react-native-server": "^0.0.5",
    "@storybook/addon-react-native-web": "^0.0.23",
    "@storybook/builder-webpack5": "^6.5",
    "@storybook/docs-tools": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/react-native": "^7.6.10",
    "@storybook/react-webpack5": "^7.6.16",
    "@storybook/theming": "^7.6.16",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "babel-loader": "^8.3.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.10",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "husky": "^8.0.0",
    "inquirer": "^9.2.12",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "storybook": "^7.6.16",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
